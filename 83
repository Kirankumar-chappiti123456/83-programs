area of rectangle:
#include <stdio.h>

int main()
{
   int area,l=1,b=2;
   area=l*b;
   printf("%d",area);
}

area of rectangle:using functions:

#include <stdio.h>
int area(int l,int b)
{
    int a=(l,b);
    return a;
}
int main()
{
   int area,l=1,b=2;
   area=l*b;
   printf("%d",area);
}

ascii values a character:using fun and with out fun :
#include <stdio.h>
int asci(int c)
{

    return c;
}
int main()
{
    char c=65;
    printf("enter the character:");
    scanf("%c",&c);
    printf("%d",c);
    
}
celsius to fahrenhiet;


#include<stdio.h>
int main()
{
    float celius,fahr;
    celius=24;
    fahr=(celius*9/5)+32;
    printf("%f",fahr);
}
swap the number :
#include<stdio.h>
int main()
{
    int a=10,b=15;
    int temp=a;
    a=b;
    b=temp;
    printf(" %d %d",b,a);
    
}

even or odd number :
#include<stdio.h>
int main()
{
    int a;
    printf("enter the number:");
    scanf("%d",&a);
    if(a%2==0)
    {
        printf("even");
    }
    
        else{
        printf("odd");
    }
}


charater is vowels or consonants:
#include<stdio.h>
int main()
{
    char c;
    printf("enter the character:");
    scanf("%c",&c);
    if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u')
    {
        printf("vowels");
    }
    else{
        printf("consonants");
    }
    }

largest number three number:
#include<stdio.h>
int main()
{
    int a=15,b=23,c=25;
    if(a>b)
    printf("a is greator");
    if(b>c)
    printf("b is greator");
    if(c>b)
    printf("c is greator");
}


leap year in c:
#include <stdio.h>

int main()
{
    int a,b;
    printf("enter the year:");
    scanf("%d",&a);
    if(a%400==0)
    {
    printf("it is a leap");
    }
    
    else{
        printf("it is not a leap year");
    }
}


postive number and negative number:
#include <stdio.h>

int main()
{
int num;
printf("enter the number:");
scanf("%d",&num);
if(num>0.0)
printf("it is a positive number");
if(num<0.0)
printf("it is a negative number");
}
factorical of a number:
#include<stdio.h>

int main(){

    int x=1,fact=1,n;

    printf("Enter a number to find factorial: ");

    scanf("%d",&n); 

    while(x<=n){
        
    

        fact=fact*x; 
        x++;
}
    printf("Factorial of %d is:%d",n,fact);

    return 0;

}

another number:
#include<stdio.h>

int main(){

    int x,fact=1,n;

    printf("Enter a number to find factorial: ");

    scanf("%d",&n); 

    for(x=1;x<=n;x++)
        
    

        fact=fact*x; 

    printf("Factorial of %d is:%d",n,fact);

    return 0;

}
