area of rectangle:
#include <stdio.h>

int main()
{
   int area,l=1,b=2;
   area=l*b;
   printf("%d",area);
}

area of rectangle:using functions:

#include <stdio.h>
int area(int l,int b)
{
    int a=(l,b);
    return a;
}
int main()
{
   int area,l=1,b=2;
   area=l*b;
   printf("%d",area);
}

ascii values a character:using fun and with out fun :
#include <stdio.h>
int asci(int c)
{

    return c;
}
int main()
{
    char c=65;
    printf("enter the character:");
    scanf("%c",&c);
    printf("%d",c);
    
}
celsius to fahrenhiet;


#include<stdio.h>
int main()
{
    float celius,fahr;
    celius=24;
    fahr=(celius*9/5)+32;
    printf("%f",fahr);
}
swap the number :
#include<stdio.h>
int main()
{
    int a=10,b=15;
    int temp=a;
    a=b;
    b=temp;
    printf(" %d %d",b,a);
    
}

even or odd number :
#include<stdio.h>
int main()
{
    int a;
    printf("enter the number:");
    scanf("%d",&a);
    if(a%2==0)
    {
        printf("even");
    }
    
        else{
        printf("odd");
    }
}


